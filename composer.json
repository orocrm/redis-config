{
    "name": "oro/redis-config",
    "type": "symfony-bundle",
    "description": "OroRedisConfigBundle",
    "homepage": "https://github.com/oroinc/redis-config",
    "license": "MIT",
    "keywords": ["Oro", "OroPlatform", "OroCommerce", "OroCRM", "Redis"],
    "require": {
        "predis/predis": "~1.0",
        "snc/redis-bundle": "~2.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*"
    },
    "autoload": {
        "psr-4": {"Oro\\Bundle\\RedisConfigBundle\\": ""},
        "exclude-from-classmap": ["/Tests/"]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    }
}
